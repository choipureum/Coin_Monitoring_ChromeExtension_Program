{
  "manifest_version": 2,

  "name": "monitoring_chromeExtension",
  "description": "monitoring my coin wallet _ choipureum  v_1.0.0",
  "version": "1.0.0",
  "author": "choipureum",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Monitoring Program"
  },
  "background": "popup.js",
  "permissions": [
    "tabs",
    "cookies",
    "storage",
    "<all_urls>"
  ]
}